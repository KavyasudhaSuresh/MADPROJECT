<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/desktop_computer"
        android:label="@string/app_name"
        android:roundIcon="@drawable/desktop_computer"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizApplication"
        tools:targetApi="31">
        <activity
            android:name=".ScoreActivity"
            android:exported="false"/>
        <activity
            android:name=".DeepLearningQuizActivity"
            android:exported="true" />
        <activity
            android:name=".R_Language_Activity"
            android:exported="false" />
        <activity
            android:name=".ComputerNetworkActivity"
            android:exported="false" />
        <activity
            android:name=".HistoryActivity"
            android:exported="false" />
        <activity
            android:name=".SignInActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".EditPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".RulesActivity"
            android:exported="true"/>
        <activity
            android:name=".QuizOptionActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false"/>
    </application>

</manifest>